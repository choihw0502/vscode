{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "src/*": ["./src/*"],
      "app/*": ["*"],

      "api/*": ["./src/api/*"],
      "assets/*": ["./src/assets/*"],
      "boot/*": ["./src/boot/*"],
      "components/*": ["./src/components/*"],
      "css/*": ["./src/css/*"],
      "layouts/*": ["./src/layouts/*"],
      "pages/*": ["./src/pages/*"],
      "router/*": ["./src/router/*"],
      "stores/*": ["./src/stores/*"],

      "vue$": ["node_modules/vue/dist/vue.runtime.esm-bundler.js"]
    },
    "jsx": "preserve"
  },
  "exclude": ["dist", ".quasar", "node_modules"]
}
